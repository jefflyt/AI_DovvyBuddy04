{
  "name": "dovvybuddy",
  "version": "0.1.0",
  "private": true,
  "description": "DovvyBuddy - AI diving assistant for certification guidance and trip planning",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run --config config/vitest.config.ts",
    "test:watch": "vitest --config config/vitest.config.ts",
    "test:integration": "vitest run tests/integration --config config/vitest.config.ts",
    "test:e2e": "playwright test --config config/playwright.config.ts",
    "test:e2e:headed": "playwright test --config config/playwright.config.ts --headed",
    "test:e2e:ui": "playwright test --config config/playwright.config.ts --ui",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "content:validate": "cd backend && python3 -m scripts.validate_content",
    "content:ingest": "cd backend && python3 -m scripts.ingest_content",
    "content:ingest:incremental": "cd backend && python3 -m scripts.ingest_content --incremental",
    "content:clear": "cd backend && python3 -m scripts.clear_embeddings",
    "content:review": "tsx scripts/review-content.ts",
    "benchmark:rag": "cd backend && python3 -m scripts.benchmark_rag"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.40.0",
    "@vercel/analytics": "^1.4.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.563.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.12.7",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
