{
  "name": "dovvybuddy",
  "version": "0.1.0",
  "private": true,
  "description": "DovvyBuddy - AI diving assistant for certification guidance and trip planning",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx --env-file=.env.local src/db/seed.ts",
    "db:clear": "tsx --env-file=.env.local src/db/clear.ts",
    "db:verify": "tsx --env-file=.env.local src/db/verify.ts",
    "content:validate": "tsx --env-file=.env.local scripts/validate-content.ts",
    "content:ingest": "tsx --env-file=.env.local scripts/ingest-content.ts",
    "content:clear": "tsx --env-file=.env.local scripts/clear-embeddings.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "gray-matter": "^4.0.3",
    "next": "^14.2.0",
    "postgres": "^3.4.7",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tiktoken": "^1.0.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
