{
  "name": "dovvybuddy",
  "version": "0.1.0",
  "private": true,
  "description": "DovvyBuddy - AI diving assistant for certification guidance and trip planning",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run tests/integration",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "content:validate": "cd src/backend && python3 -m scripts.validate_content",
    "content:ingest": "cd src/backend && python3 -m scripts.ingest_content",
    "content:ingest:incremental": "cd src/backend && python3 -m scripts.ingest_content --incremental",
    "content:clear": "cd src/backend && python3 -m scripts.clear_embeddings",
    "benchmark:rag": "cd src/backend && python3 -m scripts.benchmark_rag"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.12.7",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
